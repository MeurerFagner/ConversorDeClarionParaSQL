<Window x:Class="AplicativoAuxiliarSoftbus.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        Title="{Binding Title}" Height="350" Width="525">
    <StackPanel>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
            <Button Width="Auto" Command="{Binding NavigateCommand}" CommandParameter="FormataSentencaClarion">Converte sentença SQL</Button>
            <Button Width="Auto" Command="{Binding NavigateCommand}" CommandParameter="RegistroDeTarefas">Registro de tarefas Realizadas</Button>
        </StackPanel>
        <ContentControl prism:RegionManager.RegionName="ContentRegion" />
    </StackPanel>
</Window>
